cat("The number is", i, "and it is a multiple of three and five \n")
}
else if (i %% 3 == 0){
cat("The number is", i, "and it is a multiple of three \n")
}
else if (i %% 5 == 0){
cat("The number is", i, "and it is a multiple of five \n")
}
}
